{
  "name": "vue-router-multi-view",
  "version": "0.0.1",
  "description": "router-view meet v-show meet keep-alive",
  "main": "dist/vue-router-multi-view.umd.js",
  "module": "dist/vue-router-multi-view.esm.js",
  "unpkg": "dist/vue-router-multi-view.min.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "prepublish": "npm run build",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akryum/vue-router-multi-view.git"
  },
  "keywords": [
    "vue",
    "router"
  ],
  "author": "Guillaume Chau <guillaume.b.chau@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Akryum/vue-router-multi-view/issues"
  },
  "homepage": "https://github.com/Akryum/vue-router-multi-view#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.16.0",
    "clean-css": "^4.1.8",
    "cross-env": "^5.0.5",
    "eslint": "^4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.4.1",
    "uglify-es": "^3.0.28"
  }
}
